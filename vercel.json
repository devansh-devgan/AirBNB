{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "controllers//*.js",
        "use": "@vercel/node"
      },
      {
        "src": "init//*.js",
        "use": "@vercel/node"
      },
      {
        "src": "models//*.js",
        "use": "@vercel/node"
      },
      {
        "src": "routes//*.js",
        "use": "@vercel/node"
      },
      {
        "src": "utils//*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/",
        "use": "@vercel/static"
      },
      {
        "src": "views//*.ejs",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }